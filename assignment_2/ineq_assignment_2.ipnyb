   ],
   "source": [
    "\n",
    "\n",
    "# Compute marginal utility of consumption\n",
    "def marginal_utility(c, st):\n",
    "    if st.Î± != 1:\n",
    "        return pow(c, -st.Î±)\n",
    "    else:\n",
    "        return 1/c\n",
    "\n",
    "# Compute the multipliers mu for all t\n",
    "mu = np.zeros(st.T)\n",
    "for t in range(st.T-1):  \n",
    "    mu[t] = marginal_utility(c_path[t], st) - st.Î²*(1+st.r)*marginal_utility(c_path[t+1], st)\n",
    "\n",
    "    # If borrowing constraint is not binding, set multiplier to zero\n",
    "    if A_path[t+1] > 0:\n",
    "        mu[t] = 0\n",
    "\n",
    "# Plot the evolution of mu over the life cycle\n",
    "fig, ax = plt.subplots(figsize=(8, 5))\n",
    "ax.plot(np.arange(st.T), mu, label=\"Lagrangian Multiplier (mu)\")\n",
    "ax.grid()\n",
    "ax.set_xlabel('t')\n",
    "ax.set_ylabel('mu')\n",
    "ax.set_title('Evolution of Lagrangian Multiplier over the Life Cycle')\n",
    "plt.legend()\n",
    "plt.show()"
   ]
  },
