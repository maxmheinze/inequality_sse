    "ax.plot(np.cumsum(np.ones(st.T)), c_path, label=\"Consumption path\")\n",
    "ax.grid()\n",
    "ax.set_xlabel('t')                        #Label of x axis\n",
    "ax.set_ylabel('a, y, c')                  #Label of y axis\n",
    "plt.legend()                              #Plot the legend\n",
    "plt.show()    "
